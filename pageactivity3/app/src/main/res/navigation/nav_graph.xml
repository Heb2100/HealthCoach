<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.pageactivity3.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.pageactivity3.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">
        <action
            android:id="@+id/action_SecondFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
        <action
            android:id="@+id/action_SecondFragment_to_third2Fragment"
            app:destination="@id/third2Fragment" />
        <action
            android:id="@+id/action_SecondFragment_to_third1Fragment"
            app:destination="@id/third1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.pageactivity3.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third" >
        <action
            android:id="@+id/action_thirdFragment_to_cameraActivity"
            app:destination="@id/cameraActivity" />
        <action
            android:id="@+id/action_thirdFragment_to_camera1Activity"
            app:destination="@id/camera1Activity" />
        <action
            android:id="@+id/action_thirdFragment_to_camera2Activity"
            app:destination="@id/camera2Activity" />
    </fragment>
    <fragment
        android:id="@+id/third2Fragment"
        android:name="com.example.pageactivity3.Third2Fragment"
        android:label="fragment_third2"
        tools:layout="@layout/fragment_third2" >
        <action
            android:id="@+id/action_third2Fragment_to_camera7Activity"
            app:destination="@id/camera7Activity" />
        <action
            android:id="@+id/action_third2Fragment_to_camera6Activity"
            app:destination="@id/camera6Activity" />
        <action
            android:id="@+id/action_third2Fragment_to_camera8Activity"
            app:destination="@id/camera8Activity" />
    </fragment>
    <fragment
        android:id="@+id/third1Fragment"
        android:name="com.example.pageactivity3.Third1Fragment"
        android:label="fragment_third1"
        tools:layout="@layout/fragment_third1" >
        <action
            android:id="@+id/action_third1Fragment_to_camera3Activity"
            app:destination="@id/camera3Activity" />
        <action
            android:id="@+id/action_third1Fragment_to_camera4Activity"
            app:destination="@id/camera4Activity" />
        <action
            android:id="@+id/action_third1Fragment_to_camera5Activity"
            app:destination="@id/camera5Activity" />
    </fragment>
    <activity
        android:id="@+id/cameraActivity"
        android:name="com.example.pageactivity3.CameraActivity"
        android:label="activity_camera"
        tools:layout="@layout/activity_camera" />
    <activity
        android:id="@+id/camera1Activity"
        android:name="com.example.pageactivity3.Camera1Activity"
        android:label="activity_camera1"
        tools:layout="@layout/activity_camera1" />
    <activity
        android:id="@+id/camera2Activity"
        android:name="com.example.pageactivity3.Camera2Activity"
        android:label="activity_camera2"
        tools:layout="@layout/activity_camera2" />
    <activity
        android:id="@+id/camera3Activity"
        android:name="com.example.pageactivity3.Camera3Activity"
        android:label="activity_camera3"
        tools:layout="@layout/activity_camera3" />
    <activity
        android:id="@+id/camera4Activity"
        android:name="com.example.pageactivity3.Camera4Activity"
        android:label="activity_camera4"
        tools:layout="@layout/activity_camera4" />
    <activity
        android:id="@+id/camera5Activity"
        android:name="com.example.pageactivity3.Camera5Activity"
        android:label="activity_camera5"
        tools:layout="@layout/activity_camera5" />
    <activity
        android:id="@+id/camera6Activity"
        android:name="com.example.pageactivity3.Camera6Activity"
        android:label="activity_camera6"
        tools:layout="@layout/activity_camera6" />
    <activity
        android:id="@+id/camera7Activity"
        android:name="com.example.pageactivity3.Camera7Activity"
        android:label="activity_camera7"
        tools:layout="@layout/activity_camera7" />
    <activity
        android:id="@+id/camera8Activity"
        android:name="com.example.pageactivity3.Camera8Activity"
        android:label="activity_camera8"
        tools:layout="@layout/activity_camera8" />
</navigation>